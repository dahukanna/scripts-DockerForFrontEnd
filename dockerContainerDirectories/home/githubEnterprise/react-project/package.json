{
  "name": "react-project",
  "version": "0.0.1",
  "date": "9 June 2017",
  "description": "ReactJS template project",
  "author": "Dawn Ahukanna <dawn.n.ahukanna@ibm.com>",
  "private": true,
  "license": "Apache-2.0",
  "engines": {
    "node": "6.2.1",
    "npm": "3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:<Insert Org Name>/<Insert Repository Name>.git"
  },
  "scripts": {
    "clean": "./node_modules/.bin/del './dist/' './lib/server-compiled.js'",
    "compile:server": "babel lib --out-file lib/server-compiled.js",
    "compile:webpack": "NODE_ENV=production webpack -p",
    "compile": "npm run compile:server && npm run compile:webpack",
    "lint:sass": "./node_modules/.bin/sass-lint './src/**/*.scss' -v -q",
    "lint:javascript": "./node_modules/.bin/eslint ./src",
    "lint": "npm run lint:javascript && npm run lint:sass",
    "test": "npm run lint --silent",
    "startLocal": "nodemon ./lib/server.js --exec babel-node",
    "startLocalDebug": "DEBUG_MODE=true nodemon ./lib/server.js --exec babel-node",
    "startDebug": "NODE_ENV=production DEBUG_MODE=true node ./lib/server-compiled.js",
    "start": "NODE_ENV=production node ./lib/server-compiled.js"
  },
  "dependencies": {
    "autoprefixer": "6.6.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "config": "1.24.0",
    "connect-favicons": "0.1.5",
    "css-loader": "0.26.1",
    "del": "2.2.2",
    "eslint": "3.13.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "express": "4.13.x",
    "express-session": "1.14.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "history": "4.5.1",
    "ibm-design-colors": "2.0.3",
    "ldapjs": "1.0.1",
    "morgan": "1.7.0",
    "node-sass": "4.2.0",
    "passport": "0.3.2",
    "passport-saml": "0.15.0",
    "postcss-loader": "1.2.2",
    "precss": "1.4.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-modal": "^2.2.2",
    "react-router": "4.0.0-alpha.6",
    "request": "2.79.0",
    "sass-lint": "1.10.2",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "whatwg-fetch": "2.0.2",
    "xmldom": "0.5.0",
    "xpath": "0.0.23"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
